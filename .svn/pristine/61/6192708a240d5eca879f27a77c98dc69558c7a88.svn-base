interface BasicProps {
  required?: boolean;
  disabled?: boolean;
  label?: string;
  placeholder?: string;
  readonly?: string;
  default?: string | any;
}

interface DatePickerProps extends BasicProps {
  picker?: string;
  format?: string; //日期格式 默认 yyyy-MM-dd
  timestamp?: boolean; //是否获取时间戳
}

interface SelectProps extends BasicProps {
  mode: string;
  enum: any[];
}
